            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alburaq.simled.Panel"
            android:label="Insta-Pay"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.alburaq.simled.Party"
            android:label="@string/sales_name"
            android:theme="@style/AppTheme" >
